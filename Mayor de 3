using namespace std;

void main ()

{
	int a,b,c,mayor;

	cout<< "ingresa a";
	cin>> a;
	cout<<"ingrese b";
	cin>>b;
	cout<< "ingrese c";
	cin>> c;
	
	if (a>b)
		if(a>c)
			mayor=a;
		else 
			mayor=c;
	else
		if(b>c)
			mayor=b;
		else
			mayor=c;
	cout<<"El mayor es:"<< mayor;
	getche();
}
